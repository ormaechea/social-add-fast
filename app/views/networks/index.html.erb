
<%= link_to 'Add Network', new_user_network_path %>
<% @networks.each do |network| %>
  <p>
    <tr>
     <%= network.name %>
     <%= network.username %>
      <%= link_to 'Edit', {action: 'edit', id: network.id} %>
      <%= link_to 'Destroy', user_network_path(network.user_id, network.id), method: :delete %>

    </tr>
  </p>
<% end %>

