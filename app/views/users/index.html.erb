<%= link_to 'create user', new_user_path %>
<p>
<% @users.each do |user| %>
  <p>
    <tr>
      <td><%= link_to user.username, user_path(user) %></td>
      <td><%= link_to "edit", edit_user_path(user) %></td>
      <td><%= link_to 'Delete', user_path(user),
              method: :delete,
              data: { confirm: 'Are you sure?' } %></td>
    </tr>
  </p>
<% end %>
</p>
